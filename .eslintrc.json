{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["prettier", "react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "react/prop-types": [0],
    "no-duplicate-imports": ["error"],
    "no-import-assign": "off"
  },
  "globals": {
    "CKFinder": "readonly"
  }
}
